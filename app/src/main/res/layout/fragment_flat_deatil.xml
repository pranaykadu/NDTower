<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="in.ovtech.ndtower.FlatDetailActivity$PlaceholderFragment"
    android:orientation="vertical">


    <TextView android:id="@+id/section_label" android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView android:text="Flat Number"
            android:layout_width="@dimen/abc_text_size_display_4_material"
            android:layout_height="wrap_content"

            />
        <TextView android:text="Show Flat Number"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/fnumber_db"
            />
        <!--"@dimen/abc_text_size_display_4_material"-->
    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView android:text="Flat Type"
            android:layout_width="@dimen/abc_text_size_display_4_material"
            android:layout_height="wrap_content"

            />
        <TextView android:text="Show Flat Type"
            android:layout_width="@dimen/abc_text_size_display_4_material"
            android:layout_height="wrap_content"
            android:id="@+id/ftype_db"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView android:text="Wing"
            android:layout_width="@dimen/abc_text_size_display_4_material"
            android:layout_height="wrap_content"

            />
        <TextView android:text="Show wing"
            android:layout_width="@dimen/abc_text_size_display_4_material"
            android:layout_height="wrap_content"
            android:id="@+id/fwing_db"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView android:text="Floor"
            android:layout_width="@dimen/abc_text_size_display_4_material"
            android:layout_height="wrap_content"

            />
        <TextView android:text="Show Floor"
            android:layout_width="@dimen/abc_text_size_display_4_material"
            android:layout_height="wrap_content"
            android:id="@+id/ffloor_db"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView android:text="Possession"
            android:layout_width="@dimen/abc_text_size_display_4_material"
            android:layout_height="wrap_content"

            />
        <TextView android:text="Show Possession"
            android:layout_width="@dimen/abc_text_size_display_4_material"
            android:layout_height="wrap_content"
            android:id="@+id/fPossession_db"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView android:text="owner Name"
            android:layout_width="@dimen/abc_text_size_display_4_material"
            android:layout_height="wrap_content"

            />
        <TextView android:text="Show owner"
            android:layout_width="@dimen/abc_text_size_display_4_material"
            android:layout_height="wrap_content"
            android:id="@+id/fowaner_db"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView android:text="Status"
            android:layout_width="@dimen/abc_text_size_display_4_material"
            android:layout_height="wrap_content"

            />
        <TextView android:text="Show Floor"
            android:layout_width="@dimen/abc_text_size_display_4_material"
            android:layout_height="wrap_content"
            android:id="@+id/fStatus_db"
            />
    </LinearLayout>
</LinearLayout>
