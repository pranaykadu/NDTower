<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">


        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView android:text="Members below 5 years"
                android:layout_width="250dp"
                android:layout_height="wrap_content"

                />
            <TextView android:text="Show Count"
                android:layout_width="@dimen/abc_text_size_display_4_material"
                android:layout_height="wrap_content"
                android:id="@+id/below5_db"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView android:text="Members between 5 to 18  years"
                android:layout_width="250dp"
                android:layout_height="wrap_content"

                />
            <TextView android:text="Show Count"
                android:layout_width="@dimen/abc_text_size_display_4_material"
                android:layout_height="wrap_content"
                android:id="@+id/between5218_db"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView android:text="Members above 60"
                android:layout_width="250dp"
                android:layout_height="wrap_content"

                />
            <TextView android:text="Show Count"
                android:layout_width="@dimen/abc_text_size_display_4_material"
                android:layout_height="wrap_content"
                android:id="@+id/above60_db"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView android:text="Female count"
                android:layout_width="250dp"
                android:layout_height="wrap_content"

                />
            <TextView android:text="Show Count"
                android:layout_width="@dimen/abc_text_size_display_4_material"
                android:layout_height="wrap_content"
                android:id="@+id/cntfemale_db"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView android:text="Male Count"
                android:layout_width="250dp"
                android:layout_height="wrap_content"

                />
            <TextView android:text="Show Count"
                android:layout_width="@dimen/abc_text_size_display_4_material"
                android:layout_height="wrap_content"
                android:id="@+id/cntmale_db"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <ListView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:id="@+id/listView1"></ListView>

        </LinearLayout>



</LinearLayout>